                {
                    "id": "iModifierHotkey:Main",
                    "text": "$ModifyHotkey",
                    "type": "keymap",
                    "help": "$OpenLastMenu",
                    "ignoreConflicts": true,
                    "valueOptions": {
                        "sourceType": "ModSettingInt"
                    }
                },
                {
                    "id": "allInOneKey",
                    "text": "$SuperKey",
                    "type": "keymap",
                    "help": "$SuperKeyDesc."
                },
                {
                    "id": "bKillEssentials:Main",
                    "type": "toggle",
                    "text": "$Kill_Essential_with_drain",
                    "help": "$Kill_Essential_with_drain_Desc",
                    "valueOptions": {
                        "sourceType": "ModSettingBool",
                        "defaultValue": false
                    }
                },
                {
                    "id": "bSpamNotifications:Main",
                    "type": "toggle",
                    "text": "$Debug_notes",
                    "help": "$Debug_notes_desc",
                    "valueOptions": {
                        "sourceType": "ModSettingBool",
                        "defaultValue": false
                    }
                },
                {
                    "id": "iSuccubusLevel:Main",
                    "text": "Succubus Starting Level",
                    "type": "enum",
                    "valueOptions": {
                        "options": [
                            "Cursed a minute ago",
                            "Fledgeling (default)",
                            "Well probed",
                            "Expert Succer",
                            "Master of the Arts",
                            "Legendary"
                        ],
                        "sourceType": "ModSettingInt",
                        "defaultValue": 1
                    }
                },
                {
                    "id": "iSpellsAdded:Main",
                    "text": "Abilities as Spells",
                    "type": "enum",
                    "help": "Choose if the abilities also appear as spells or not.",
                    "valueOptions": {
                        "options": [
                            "Only as abilities",
                            "Both",
                            "Only as spells"
                        ],
                        "sourceType": "ModSettingInt"
                    },
                    "action": {
                        "type": "CallFunction",
                        "form": "TintsOfASuccubusSecretDesires.esp|001",
                        "scriptName": "tssd_actions",
                        "function": "toggleSpells",
                        "params": [
                            "{i}-1"
                        ]
                    }
                }
            ]
        },
